div
- ind = 0
	p= the_test[seq[ind]].question
	form()
		each option in the_test[seq[ind]].options
			input(type='radio', value='#{option}', name='submission')
			span(style='padding: 1em')= option
			br
		button(type='button', onclick='next_question()') Next



script.
	function next_question() {
		ind = ind + 1;
		console.log(ind);
		return ind
	}


